{
    "compilerOptions": {
      "target": "es5",                               
      "module": "commonjs",                           
      "allowJs": false,                             
      "outDir": "./dist",         
      "strict": true,                                 
      "strictPropertyInitialization": false,        
      "esModuleInterop": true,                        
      "experimentalDecorators": true,              
      "emitDecoratorMetadata": true,                
      "skipLibCheck": true,                           
      "forceConsistentCasingInFileNames": true   
    }
  }   